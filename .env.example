HOST=127.0.0.1
ALLOW_REMOTE_DASHBOARD=false
TRUST_PROXY_HOPS=0
STRICT_LIVE_QUOTA=false
PORT=1455
DATA_FILE=~/.omni-connector/data/store.json
SESSION_SECRET=
SESSION_SECRET_FILE=~/.omni-connector/data/store.json.session
SESSION_STORE=memorystore
# Generate with: npm run generate:data-key
DATA_ENCRYPTION_KEY=
PUBLIC_DIR=./public

# Optional overrides for built-in OpenAI Codex OAuth defaults
OAUTH_PROVIDER_NAME=OpenAI
OAUTH_AUTHORIZATION_URL=https://auth.openai.com/oauth/authorize
OAUTH_TOKEN_URL=https://auth.openai.com/oauth/token
OAUTH_CLIENT_ID=app_EMoamEEZ73f0CkXaXp7hrann
OAUTH_CLIENT_SECRET=
OAUTH_SCOPES=openid profile email offline_access
OAUTH_REDIRECT_URI=http://localhost:1455/auth/callback
OAUTH_ORIGINATOR=pi
CODEX_APP_SERVER_ENABLED=true
CODEX_APP_SERVER_COMMAND=codex
CODEX_APP_SERVER_ARGS=app-server
CODEX_APP_SERVER_TIMEOUT_MS=3500

# Gemini OAuth profiles
# Auto-discover Gemini OAuth credentials from installed @google/gemini-cli bundle
# and Antigravity/OpenClaw OAuth bundles when available
# (disable only if you want strict env-only config)
GEMINI_OAUTH_AUTO_DISCOVER=true

# Option 1: Gemini CLI
GEMINI_CLI_OAUTH_CLIENT_ID=
GEMINI_CLI_OAUTH_CLIENT_SECRET=
GEMINI_CLI_OAUTH_AUTHORIZATION_URL=https://accounts.google.com/o/oauth2/v2/auth
GEMINI_CLI_OAUTH_TOKEN_URL=https://oauth2.googleapis.com/token
GEMINI_CLI_OAUTH_USERINFO_URL=https://openidconnect.googleapis.com/v1/userinfo
GEMINI_CLI_OAUTH_SCOPES=https://www.googleapis.com/auth/cloud-platform https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/userinfo.profile

# Option 2: Antigravity
GEMINI_ANTIGRAVITY_OAUTH_CLIENT_ID=
GEMINI_ANTIGRAVITY_OAUTH_CLIENT_SECRET=
GEMINI_ANTIGRAVITY_OAUTH_AUTHORIZATION_URL=https://accounts.google.com/o/oauth2/v2/auth
GEMINI_ANTIGRAVITY_OAUTH_TOKEN_URL=https://oauth2.googleapis.com/token
GEMINI_ANTIGRAVITY_OAUTH_USERINFO_URL=https://openidconnect.googleapis.com/v1/userinfo
GEMINI_ANTIGRAVITY_OAUTH_SCOPES=https://www.googleapis.com/auth/cloud-platform https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/cclog https://www.googleapis.com/auth/experimentsandconfigs

# Optional endpoints if your provider exposes them
OAUTH_USERINFO_URL=
OAUTH_QUOTA_URL=
OAUTH_REQUIRE_QUOTA=true

DEFAULT_FIVE_HOUR_LIMIT=0
DEFAULT_FIVE_HOUR_USED=0
DEFAULT_WEEKLY_LIMIT=0
DEFAULT_WEEKLY_USED=0

# Provider inference endpoints used by /v1/chat/completions routing
CODEX_INFERENCE_BASE_URL=https://api.openai.com/v1
CODEX_CHATGPT_BASE_URL=https://chatgpt.com/backend-api/codex
GEMINI_INFERENCE_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai
CLAUDE_INFERENCE_BASE_URL=https://api.anthropic.com/v1
OPENROUTER_INFERENCE_BASE_URL=https://openrouter.ai/api/v1

# Official provider usage adapters (optional).
# Strict mode requires live usage for routing, so configure these per provider.
#
# For Gemini auth in your environment, common options are Antigravity or Gemini CLI credentials,
# but usage polling here needs an HTTP-usable API key/token for configured endpoints.

# CODEX_USAGE_PARSER=openai_usage
# CODEX_USAGE_AUTH_MODE=bearer
